/* end confdefs.h.  */

         #include <unistd.h>
#include <fcntl.h>
#include <errno.h>
int
main ()
{
int result = 0;
#ifdef FD_CLOEXEC
            if (fcntl (1, F_SETFD, FD_CLOEXEC) == -1)
              result |= 1;
#endif
            if (dup2 (1, 1) == 0)
              result |= 2;
#ifdef FD_CLOEXEC
            if (fcntl (1, F_GETFD) != FD_CLOEXEC)
              result |= 4;
#endif
            close (0);
            if (dup2 (0, 0) != -1)
              result |= 8;
            /* Many gnulib modules require POSIX conformance of EBADF.  */
            if (dup2 (2, 1000000) == -1 && errno != EBADF)
              result |= 16;
            /* Flush out a cygwin core dump.  */
            if (dup2 (2, -1) != -1 || errno != EBADF)
              result |= 32;
            return result;

  ;
  return 0;
}

