# When compiling with GCC on some OSs (Solaris, AIX), _Complex_I doesn't
# work; in the reported cases so far, 1.0fi works well instead.  According
# to the C99 spec, the complex.h header must provide a working definition
# of _Complex_I, so we always try _Complex_I first.  The 1.0fi fallback
# is a workaround for the failure of some systems to conform to C99.
if test "$ac_cv_type_complex_double" = yes; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for i" >&5
$as_echo_n "checking for i... " >&6; }
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#if HAVE_COMPLEX_H
#include <complex.h>
#endif
complex double z;

int
main ()
{

z = _Complex_I;

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :

$as_echo "#define GUILE_I _Complex_I" >>confdefs.h

     { $as_echo "$as_me:${as_lineno-$LINENO}: result: _Complex_I" >&5
$as_echo "_Complex_I" >&6; }
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#if HAVE_COMPLEX_H
#include <complex.h>
#endif
complex double z;

int
main ()
{

z = 1.0fi;

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  $as_echo "#define GUILE_I 1.0fi" >>confdefs.h

   { $as_echo "$as_me:${as_lineno-$LINENO}: result: 1.0fi" >&5
$as_echo "1.0fi" >&6; }
