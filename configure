  ac_cv_type_complex_double=no
   { $as_echo "$as_me:${as_lineno-$LINENO}: result: not available" >&5
$as_echo "not available" >&6; }
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
fi
